{
	"name": "chat-worker",
	"main": "src/index.ts",
	"compatibility_date": "2025-12-07",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
	  "enabled": true,
	  "head_sampling_rate": 1
	},

	"assets": {
		"directory": "./public",
		"binding": "ASSETS"
	},

	"durable_objects": {
	  "bindings": [
		{
		  "name": "CHAT_ROOM",
		  "class_name": "ChatRoom"
		}
	  ]
	},

	"migrations": [
	  {
		"tag": "v1",
		"new_sqlite_classes": ["ChatRoom"]
	  }
	],

	"ai": {
		"binding": "AI"
	}

  }